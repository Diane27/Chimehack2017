                                 <div class="checkbox">
                                    <label for="useageboxes-0">
                                    <input type="checkbox" name="useageboxes" id="useageboxes-0" value="">
                                    Academics
                                    </label>
                                 </div>
                                 <div class="checkbox">
                                    <label for="useageboxes-1">
                                    <input type="checkbox" name="useageboxes" id="useageboxes-1" value="">
